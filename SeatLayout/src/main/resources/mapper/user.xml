<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myway.seat.api.mapper.UserMapper">
    <select id="findUserByAccountIdXml" resultType="User">
        SELECT * FROM user WhERE account_id = #{userId}
    </select>
    
    <insert id="saveUserXml" parameterType="com.myway.seat.api.model.param.UserParam">
   	 insert into user(account_id,user_nm,enabled) VALUES(#{accountId},#{userNm},true);

    </insert>
</mapper>